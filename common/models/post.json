      "required": true,
      "length": 5000
    },
    "created": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "edited": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
